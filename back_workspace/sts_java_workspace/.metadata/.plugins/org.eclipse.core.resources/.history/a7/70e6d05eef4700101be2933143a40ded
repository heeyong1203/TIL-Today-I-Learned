package com.sinse.shopadmin.product.view;

public class ExceptionTest2 {
	public void test() {
		
	}
	
	public void test2() {
		String s = "100k";
		
		try {
			Integer.parseInt(s);
		} catch (NumberFormatException e) {
			System.out.println("숫자로 변경될 수 없는 문자가 있습니다.");
			e.printStackTrace();
		}
	}
	
	public static void main(String[] args) {
		new ExceptionTest2().test2();
	}
}
